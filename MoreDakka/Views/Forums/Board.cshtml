@{
    Layout = null;
    
}
<div class="container">
    <div style="padding: 20px 0 20px;">
        <span style="font-size: 3.0em;" data-ng-click="goToForum()" class="glyphicon glyphicon-circle-arrow-left"></span>
    </div>
    <table class="table table-striped table-bordered table-hover">
        <thead>
            <tr>
                <th></th>
                <th>Topic Name</th>
                <th>Posts</th>
                <th>Last Post</th>
            </tr>
        </thead>
        <tbody>
            <tr data-ng-repeat="topic in topics">
                <td></td>
                <td data-ng-click="openTopic(topic.id)">{{ topic.name }}</td>
                <td>0</td>
                <td>None</td>
            </tr>
        </tbody>
    </table>
</div>