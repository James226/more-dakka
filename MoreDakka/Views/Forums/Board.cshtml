@{
    Layout = null;
    
}
<div class="container">
    <div class="panel" id="forum-panel">
        <div class="panel-body panel-default">
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th></th>
                        <th>Topic Name</th>
                        <th>Posts</th>
                        <th>Last Post</th>
                    </tr>
                </thead>
                <tbody>
                    <tr data-ng-repeat="topic in topics">
                        <td></td>
                        <td data-ng-click="openTopic(topic.id)">{{ topic.name }}</td>
                        <td>0</td>
                        <td>None</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>